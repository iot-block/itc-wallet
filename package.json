{
  "name": "itc-wallet",
  "productName": "itc-wallet",
  "version": "0.0.1",
  "description": "IoT Chain wallet",
  "author": "iotchain",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "node .electron-vue/build.js && electron-builder -w",
    "test": "npm run unit && npm run e2e",
    "e2e": "npm run pack && mocha test/e2e",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "unit": "karma start test/unit/karma.conf.js",
    "fixp": "rm -rf node_modules/scrypt",
    "fixadd": "yarn run fixp && yarn",
    "fixusb": "electron-builder install-app-deps",
    "packager": "electron-packager ./ VEA_L --platform=win32 --arch=x64 --electron-version=2.0.0 --overwrite"
  },
  "main": "./dist/electron/main.js",
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.2",
    "@ledgerhq/hw-app-eth": "^4.76.0",
    "@ledgerhq/hw-transport-node-hid": "^4.38.6",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "electron-debug": "^1.5.0",
    "electron-store": "^5.1.0",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-wallet": "^0.6.3",
    "iotchain-js-sdk": "^1.0.0",
    "keythereum": "^1.0.4",
    "vue": "^2.6.11",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^8.15.3",
    "vue-router": "^3.1.5",
    "vuetify": "^2.2.8",
    "web3-eth-accounts": "^1.2.6",
    "web3-utils": "^1.2.6"
  },
  "devDependencies": {
    "@mdi/font": "^4.8.95",
    "ajv": "^6.11.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.5.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^18.3.7",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.2",
    "karma-electron": "^6.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^3.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "mini-css-extract-plugin": "0.4.0",
    "mocha": "^5.2.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "require-dir": "^1.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "spectron": "^3.8.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.8.3",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.10.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "build": {
    "productName": "itc-wallet",
    "appId": "io.iotchain.wallet",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "license": null
}
